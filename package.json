{
    "name": "libx.js",
    "version": "3.6.0",
    "description": "",
    "main": "./build/bundles/essentials",
    "types": "./globals.d.ts",
    "scripts": {
        "clean": "rm -rf build/;",
        "build": "yarn clean; tsc",
        "watch": "yarn clean; tsc -w",
        "main": "node build/Main.js",
        "bundle": "yarn bundle:require && yarn bundle:libx && du -sh dist/*",
        "bundle:require": "ts-node tools/browserify.ts --filename='require.libx.min' --input=\"./build/browser/require.js\" --minify",
        "bundle:libx": "ts-node tools/browserify.ts --filename='libx.min' --minify",
        "serve": "nodemon --watch 'tests/modules/mockServer.ts' --exec tsx --inspect tests/modules/mockServer.ts",
        "test": "jest",
        "test:debug": "node --inspect ./node_modules/jest/bin/jest.js --runInBand --colors --verbose",
        "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
        "bump": "./bump.sh"
    },
    "files": [
        "dist/**",
        "build/**",
        "bundles/**",
        "browser/**",
        "node/**",
        "modules/**",
        "tools/**",
        "src/**",
        "*.js",
        "*.ts"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.21.3",
        "concurrency.libx.js": "^1.2.1",
        "cors": "^2.8.5",
        "crypto-js": "^4.0.0",
        "detect.js": "^2.2.3",
        "di.libx.js": "^2.3.2",
        "form-data": "^4.0.0",
        "glob": "^7.2.0",
        "lodash": "^4.17.21",
        "prompt": "^1.1.0",
        "rxjs": "^6.6.3",
        "rxjs-compat": "^6.6.7",
        "web-streams-polyfill": "^3.2.1",
        "xregexp": "^5.0.2",
        "yargs": "^17.2.1"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/express": "^4.17.13",
        "@types/express-formidable": "^1.2.0",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.171",
        "@types/node": "18",
        "@types/xregexp": "^4.3.0",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "firebase": "^9.6.7",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "jest-junit": "^14.0.1",
        "jest-junit-reporter": "^1.1.0",
        "node-fetch": "2",
        "nodemon": "^3.0.2",
        "pax.libx.js": "^0.6.17",
        "ts-jest": "^29.0.1",
        "ts-node": "^10.1.0",
        "typescript": "^4.5.4"
    },
    "not-in-use-dependencies": {
        "bignumber.js": "^8.0.1",
        "bn.js": "^4.11.8",
        "bs58": "^4.0.1",
        "core-js": "2",
        "cryptico": "^1.0.2",
        "elliptic": "^6.5.3",
        "install": "^0.12.2",
        "jquery": "^3.3.1",
        "npm": "^6.9.0"
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "output": "./.tmp/test/TEST-RESULTS.xml",
        "classNameTemplate": "{classname} - {title}",
        "titleTemplate": "{classname} - {title}",
        "ancestorSeparator": " > ",
        "usePathForSuiteName": "true"
    }
}
