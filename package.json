{
    "name": "libx.js",
    "version": "1.0.7",
    "description": "",
    "main": "./src/bundles/essentials",
    "scripts": {
        "clean": "rm -rf build/;",
        "build": "yarn clean; tsc",
        "watch": "yarn clean; tsc -w",
        "main": "node build/Main.js",
        "bundle": "ts-node tools/browserify.ts --filename='libx.min' --minify && du -sh dist/libx.min.js",
        "test": "jest",
        "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
        "bump": "./bump.sh"
    },
    "types": "src/globals.d.ts",
    "files": [
        "dist/**",
        "build/**",
        "bundles/**",
        "compiled/**",
        "browser/**",
        "node/**",
        "modules/**",
        "src/**",
        "*.js",
        "*.ts"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.20.0",
        "concurrency.libx.js": "^0.2.25",
        "crypto-js": "^4.0.0",
        "di.libx.js": "^0.1.5",
        "json-bump": "^1.0.2",
        "lodash": "^4.17.20",
        "prompt": "^1.0.0",
        "rxjs": "^6.6.3",
        "xregexp": "^4.3.0"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.1",
        "@types/jest": "^26.0.14",
        "@types/lodash": "^4.14.161",
        "@types/node": "^13.7.1",
        "@types/xregexp": "^4.3.0",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "form-data": "^3.0.0",
        "jest": "^25.1.0",
        "jest-junit": "^10.0.0",
        "jest-junit-reporter": "^1.1.0",
        "pax.libx.js": "^0.3.4",
        "ts-jest": "^25.2.0",
        "ts-node": "^9.0.0",
        "typescript": "^3.9.3"
    },
    "not-in-use-dependencies": {
        "bignumber.js": "^8.0.1",
        "bn.js": "^4.11.8",
        "bs58": "^4.0.1",
        "core-js": "2",
        "cryptico": "^1.0.2",
        "elliptic": "^6.5.3",
        "install": "^0.12.2",
        "jquery": "^3.3.1",
        "npm": "^6.9.0"
    },
    "jest": {
        "coverageReporters": [
            "cobertura",
            "html"
        ]
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "output": "./.tmp/test/TEST-RESULTS.xml",
        "classNameTemplate": "{classname} - {title}",
        "titleTemplate": "{classname} - {title}",
        "ancestorSeparator": " > ",
        "usePathForSuiteName": "true"
    }
}
