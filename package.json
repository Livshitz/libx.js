{
  "name": "libx.js",
  "version": "4.6.1",
  "description": "",
  "main": "./build/bundles/essentials",
  "types": "./globals.d.ts",
  "scripts": {
    "clean": "rm -rf build/;",
    "build:esm": "tsc --module ESNext --outDir build/esm",
    "build:cjs": "tsc --module CommonJS",
    "build": "bun run clean; bun run build:esm && bun run build:cjs",
    "watch": "bun run clean; tsc -w",
    "main": "node build/Main.js",
    "bundle": "bun run build && bun run bundle:require && bun run bundle:libx && du -sh dist/*",
    "bundle:require": "tsx tools/browserify.ts --filename='require.libx.min' --input=\"./build/browser/require.js\" --minify",
    "bundle:libx": "node --import tsx tools/browserify.ts --filename='libx.min' --minify",
    "serve": "nodemon --watch 'tests/modules/mockServer.ts' --exec tsx --inspect tests/modules/mockServer.ts",
    "test": "jest",
    "test:debug": "node --inspect ./node_modules/.bin/jest --runInBand --colors --verbose",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
    "bump": "./bump.sh"
  },
  "files": [
    "dist/**",
    "build/**",
    "bundles/**",
    "browser/**",
    "node/**",
    "modules/**",
    "tools/**",
    "src/**",
    "*.js",
    "*.ts"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.3",
    "concurrency.libx.js": "^1.2.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "detect-browser": "^5.3.0",
    "di.libx.js": "^2.3.2",
    "form-data": "^4.0.0",
    "glob": "^7.2.0",
    "lodash-es": "^4.17.21",
    "prompt": "^1.1.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.6.7",
    "web-streams-polyfill": "^3.2.1",
    "xregexp": "^5.0.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/express-formidable": "^1.2.0",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "18",
    "@types/xregexp": "^4.3.0",
    "caniuse-lite": "^1.0.30001687",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "firebase": "^9.6.7",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.0.3",
    "jest-junit": "^14.0.1",
    "jest-junit-reporter": "^1.1.0",
    "node-fetch": "2",
    "nodemon": "^3.0.2",
    "pax.libx.js": "^0.6.27",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5"
  },
  "not-in-use-dependencies": {
    "bignumber.js": "^8.0.1",
    "bn.js": "^4.11.8",
    "bs58": "^4.0.1",
    "core-js": "2",
    "cryptico": "^1.0.2",
    "elliptic": "^6.5.3",
    "install": "^0.12.2",
    "jquery": "^3.3.1",
    "npm": "^6.9.0"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./.tmp/test/TEST-RESULTS.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
